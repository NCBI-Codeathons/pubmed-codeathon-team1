        <ArticleTitle>Tularaemia - a diagnostic challenge.</ArticleTitle>
        <Pagination>
          <StartPage>12</StartPage>
          <EndPage>21</EndPage>
          <MedlinePgn>12-21</MedlinePgn>
        </Pagination>
